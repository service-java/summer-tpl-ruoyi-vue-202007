<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.ctag.mapper.CtagOssProjectMapper">

    <resultMap type="CtagOssProject" id="CtagOssProjectResult">
        <result property="id"    column="id"    />
        <result property="ossId"    column="oss_id"    />
        <result property="projectId"    column="project_id"    />
    </resultMap>

    <sql id="selectCtagOssProjectVo">
        select id, oss_id, project_id from ctag_oss_project
    </sql>

    <select id="selectCtagOssProjectList" parameterType="CtagOssProject" resultMap="CtagOssProjectResult">
        <include refid="selectCtagOssProjectVo"/>
        <where>
            <if test="ossId != null "> and oss_id = #{ossId}</if>
            <if test="projectId != null "> and project_id = #{projectId}</if>
        </where>
    </select>

    <select id="selectCtagOssProjectById" parameterType="Long" resultMap="CtagOssProjectResult">
        <include refid="selectCtagOssProjectVo"/>
        where id = #{id}
    </select>

    <insert id="insertCtagOssProject" parameterType="CtagOssProject" useGeneratedKeys="true" keyProperty="id">
        insert into ctag_oss_project
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="ossId != null">oss_id,</if>
            <if test="projectId != null">project_id,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="ossId != null">#{ossId},</if>
            <if test="projectId != null">#{projectId},</if>
         </trim>
    </insert>

    <update id="updateCtagOssProject" parameterType="CtagOssProject">
        update ctag_oss_project
        <trim prefix="SET" suffixOverrides=",">
            <if test="ossId != null">oss_id = #{ossId},</if>
            <if test="projectId != null">project_id = #{projectId},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteCtagOssProjectById" parameterType="Long">
        delete from ctag_oss_project where id = #{id}
    </delete>

    <delete id="deleteCtagOssProjectByIds" parameterType="String">
        delete from ctag_oss_project where id in
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>

</mapper>
